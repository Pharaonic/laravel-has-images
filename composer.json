{
    "name": "pharaonic/laravel-has-images",
    "description": "Laravel-Has-Images provides a quick and easy way to link images with a model.",
    "keywords": [
        "laravel images",
        "laravel files",
        "php images",
        "php files",
        "images",
        "laravel",
        "php"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Moamen Eltouny (Raggi)",
            "email": "raggi@raggitech.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "laravel/framework": ">=6.0",
        "pharaonic/laravel-uploader": ">=1.0",
        "pharaonic/laravel-helpers": ">=1.0"
    },
    "autoload": {
        "psr-4": {
            "Pharaonic\\Laravel\\Images\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pharaonic\\Laravel\\Images\\ImagesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
